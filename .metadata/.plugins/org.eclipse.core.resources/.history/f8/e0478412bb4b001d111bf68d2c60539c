public class Constant {
	enum Category{
		CARD,
		GIFTWARE,
		STATIONERY,
		BOOK,
		NEWSPAPERS,
		MAGAZINE
	}
	
	enum PaymentType{
		DEBITCARD,
		CREDITCARD,
		COD,
		TRANSFER
	}
	
	enum OrderStatus{
		RECEIVED,
		PROCESSING,
		READY,
		FINISHED
	}
	
	enum PhoneNumberType{
		MOBILE,
		HOME,
		WORK
	}
}
