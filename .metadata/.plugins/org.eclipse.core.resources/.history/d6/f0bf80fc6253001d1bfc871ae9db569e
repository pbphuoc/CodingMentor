package day6;

public class Book {
	private String ISBN;
	private String name;
	private String author;
	private String typeID;

	public Book(String ISBN, String name, String author, String typeID) {
		this.ISBN = ISBN;
		this.name = name;
		this.author = author;
		this.typeID = typeID;
	}

	public String getISBN() {
		return ISBN;
	}

	public String getName() {
		return name;
	}

	public String getAuthor() {
		return author;
	}

	public String getTypeID() {
		return typeID;
	}
}
