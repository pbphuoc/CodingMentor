	public static void sortStringArr(String[] strings, int first, int last) {
		if(first == last - 1)
		{
			
		}else if(first < last) {
			sortStringArr(strings, first, (first+last)/2);
			sortStringArr(strings, 1 + (first+last)/2, last);
			mergeStringArr(strings, first, first+last)/2, last);
		}

	}
}
